# Lights
genfscon sysfs /devices/virtual/sec/led                              u:object_r:sysfs_leds:s0
genfscon sysfs /class/backlight/panel0-backlight/brightness          u:object_r:sysfs_leds:s0
genfscon sysfs /class/backlight/panel0-backlight/max_brightness      u:object_r:sysfs_leds:s0

# Sec Key
genfscon sysfs /devices/virtual/sec/sec_key                                       u:object_r:sysfs_sec_key:s0
